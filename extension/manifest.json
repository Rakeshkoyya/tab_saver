{
	"manifest_version": 3,
	"name": "TabSaver",
	"version": "1.0.0",

	"description": "Export and Import your Tabs.",
	"homepage_url": "https://github.com/rakeshkoyya/Save-Tabs",

	"icons": {
		"16": "icons/Save_Tabs_16.png",
		"32":"icons/icon_32.png"
	},

	"permissions": [
		"tabs",
		"tabGroups",
		"downloads",
		"storage"
	],

	"incognito": "split",
	
	"background": {
		"service_worker": "background.js"
	},

	"action": {
		"default_icon": {
			"16": "icons/icon_16.png",
			"32": "icons/icon_32.png",
			"48": "icons/icon_48.png",
			"64": "icons/icon_64.png",
			"96": "icons/icon_96.png",
			"128": "icons/icon_128.png"
		},
		"default_title": "TabSaver",
		"default_popup": "saveTab.html"
	}
}